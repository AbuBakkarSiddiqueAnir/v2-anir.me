---

<Layout title="Project archive">
  <section class="w-full px-12">
    <div class="mx-auto my-12 max-w-[80rem]">
      <a class="flex items-center" href="/">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          viewBox="0 0 20 20"
          fill="#64ffda"
          class="mr-1 h-4 w-4 rotate-180 transition-transform group-hover:-translate-x-2"
          aria-hidden="true"
        >
          <path
            fill-rule="evenodd"
            d="M3 10a.75.75 0 01.75-.75h10.638L10.23 5.29a.75.75 0 111.04-1.08l5.5 5.25a.75.75 0 010 1.08l-5.5 5.25a.75.75 0 11-1.04-1.08l4.158-3.96H3.75A.75.75 0 013 10z"
            clip-rule="evenodd"></path>
        </svg>
        <span class="text-green">Home</span>
      </a>
      <h2 class="text-h-lg text-green">Projects Archive</h2>
      <table id="content" class="mt-12 w-full border-collapse text-left">
        <thead
          class="/10 sticky top-16 z-10 border-b border-transparent bg-transparent px-6 py-5 backdrop-blur"
        >
          <tr>
            <th class="py-4 pr-8 text-sm font-semibold text-white">Year</th>
            <th class="py-4 pr-8 text-sm font-semibold text-white">Project</th>
            <th
              class="hidden py-4 pr-8 text-sm font-semibold text-white lg:table-cell"
              >Made at</th
            >
            <th
              class="hidden py-4 pr-8 text-sm font-semibold text-white lg:table-cell"
              >Built with</th
            >
            <th
              class="hidden py-4 pr-8 text-sm font-semibold text-white sm:table-cell"
              >Link</th
            >
          </tr>
        </thead>
        <tbody>
          {
            projects.map((project) => (
              <tr class="border-transparent-300/10 border-b last:border-none">
                <td class="py-4 pr-4 align-top text-sm">
                  <div class="trantransparent-y-px text-white">
                    {project.year}
                  </div>
                </td>
                <td class="py-4 pr-4 align-top font-semibold leading-snug text-white">
                  <div class="trantransparent-y-px whitespace-nowrap text-b-2 text-white">
                    {project.title}
                  </div>
                </td>
                <td class="hidden py-4 pr-4 align-top text-sm lg:table-cell">
                  <div class="trantransparent-y-px whitespace-nowrap text-white">
                    {project.made_at}
                  </div>
                </td>
                <td class="hidden py-4 pr-4 align-top lg:table-cell">
                  <ul class="-trantransparent-y-1.5 flex flex-wrap">
                    {project.built_with.map((t) => (
                      <li class="my-1 mr-1.5">
                        <div class="flex items-center rounded-full bg-teal-400/10 px-3 py-1 text-xs font-medium leading-5 text-teal-300">
                          {t}
                        </div>
                      </li>
                    ))}
                  </ul>
                </td>
                <td class="hidden py-4 align-top sm:table-cell">
                  <ul class="trantransparent-y-1">
                    {
                      project.external_link &&     <li class="mb-1 flex items-center">
                      <a
                        class="text-transparent-400 group/link inline-flex items-baseline text-sm text-sm font-medium leading-tight text-white hover:text-teal-300 hover:text-white focus-visible:text-teal-300 focus-visible:text-teal-300"
                        href="https://www.emersoncollective.com/"
                        target="_blank"
                        rel="noreferrer noopener"
                        aria-label="emersoncollective.com (opens in a new tab)"
                      >
                        <span>
                          <span class="inline-block">
                            {project.title}
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              viewBox="0 0 20 20"
                              fill="currentColor"
                              class="group-hover/link:-trantransparent-y-1 group-hover/link:trantransparent-x-1 group-focus-visible/link:-trantransparent-y-1 group-focus-visible/link:trantransparent-x-1 ml-0.5 inline-block h-4 w-4 shrink-0 transition-transform motion-reduce:transition-none"
                              aria-hidden="true"
                            >
                              <path
                                fill-rule="evenodd"
                                d="M5.22 14.78a.75.75 0 001.06 0l7.22-7.22v5.69a.75.75 0 001.5 0v-7.5a.75.75 0 00-.75-.75h-7.5a.75.75 0 000 1.5h5.69l-7.22 7.22a.75.75 0 000 1.06z"
                                clip-rule="evenodd"
                              />
                            </svg>
                          </span>
                        </span>
                      </a>
                    </li>
                    }
                
                  </ul>
                </td>
              </tr>
            ))
          }
        </tbody>
      </table>
    </div>
  </section>
</Layout>

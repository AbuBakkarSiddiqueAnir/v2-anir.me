    <div>
      <!-- <Logo/> -->
      <h4 class="text-h-lg text-lightest-slate">AA</h4>
    </div>
    <ul class="flex gap-x-5">
      {
        config.navLinks.map((link) => (
          <li class="inter text-b-3 font-light  text-light-slate transition-colors hover:text-green">
            {" "}
            <a href={link.url}>{link.name}</a>
          </li>
        ))
      }
    </ul>
  </div>
